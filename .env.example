# Example environment variables
# Copy this file to .env.local and add your actual API keys

# AnyRouter API Key
ANYROUTER_API_KEY=sk-your_anyrouter_key_here

# Gemini API Key
GEMINI_API_KEY=your_gemini_key_here

# OpenAI API Key
OPENAI_API_KEY=sk-your_openai_key_here

# Qwen API Key (Alibaba Cloud DashScope)
QWEN_API_KEY=sk-your_qwen_key_here

# Claude API Key (for fallback/testing)
CLAUDE_API_KEY=sk-ant-your_claude_key_here

# Timeout settings
DEFAULT_TIMEOUT_MS=15000
REQUEST_TIMEOUT_MS=30000

# Base URLs (optional overrides)
ANYROUTER_BASE_URL=https://anyrouter.top
GEMINI_BASE_URL=https://generativelanguage.googleapis.com
OPENAI_BASE_URL=https://api.openai.com
QWEN_BASE_URL=https://dashscope.aliyuncs.com